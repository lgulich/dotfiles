cmd - h : yabai -m window --focus west
cmd - j : yabai -m window --focus south
cmd - k : yabai -m window --focus north
cmd - l [
  "Brave Browser" ~
  *     : yabai -m window --focus east
]

cmd - left : yabai -m window --focus west
cmd - down : yabai -m window --focus south
cmd - up : yabai -m window --focus north
cmd - right : yabai -m window --focus east

shift + cmd - h : yabai -m window --warp west
shift + cmd - j : yabai -m window --warp south
shift + cmd - k : yabai -m window --warp north
shift + cmd - l : yabai -m window --warp east

shift + cmd - left : yabai -m window --warp west
shift + cmd - down : yabai -m window --warp south
shift + cmd - up : yabai -m window --warp north
shift + cmd - right : yabai -m window --warp east

shift + cmd - 1 : yabai -m window --space term
shift + cmd - 2 : yabai -m window --space browser
shift + cmd - 3 : yabai -m window --space code
shift + cmd - 4 : yabai -m window --space compile
# The keybinding is already used by screenshot utility on macos:
# shift + cmd - 5 : yabai -m window --space varia1
shift + cmd - 6 : yabai -m window --space varia2
shift + cmd - 7 : yabai -m window --space calendar
shift + cmd - 8 : yabai -m window --space organisation
shift + cmd - 9 : yabai -m window --space mail
shift + cmd - 0 : yabai -m window --space chat

